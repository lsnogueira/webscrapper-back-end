{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "quotemark": false,
      "object-literal-sort-keys": false,
      "trailing-comma": [
        false,
        {
          "singleline": "never",
          "multiline": {
            "objects": "never",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          }
        }
      ],
      "ordered-imports": false
    },
    "rulesDirectory": []
  }
  